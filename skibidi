local Farm500Toggle = Tabs.Farm:AddToggle("Farm500Toggle", {
    Title = "Farm 500",
    Default = false
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

local requiredToolName = "Package"
local questGiver = workspace.ChoKhach2System.KhongXoa2:WaitForChild("GetJobPart")
local completePart = workspace.ChoKhach2System.KhongXoa2:WaitForChild("TargetPart")

local function teleportTo(part)
    if part and hrp then
        hrp.CFrame = part.CFrame + Vector3.new(0, -1, 0)
    end
end

local function triggerPrompt(part)
    local prompt = part:FindFirstChildWhichIsA("ProximityPrompt", true)
    if prompt then
        fireproximityprompt(prompt)
        return true
    end
    return false
end

local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Enabled = false
screenGui.DisplayOrder = -1

local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(1,0,1,0)
frame.Position = UDim2.new(0,0,0,0)
frame.BackgroundColor3 = Color3.new(0,0,0)
frame.BackgroundTransparency = 0
frame.ZIndex = 0

local label = Instance.new("TextLabel", frame)
label.AnchorPoint = Vector2.new(0.5,0.5)
label.Position = UDim2.new(0.5,0,0.5,0)
label.Size = UDim2.new(1,0,0.2,0)
label.BackgroundTransparency = 1
label.Text = "ƒêang Farm..."
label.TextColor3 = Color3.new(1,1,1)
label.TextScaled = true
label.Font = Enum.Font.GothamBold
label.ZIndex = 1

screenGui.Parent = player:WaitForChild("PlayerGui")

local originalCFrame = nil
local farmActive = false

local function returnToOriginal()
    if originalCFrame and hrp then
        local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        local tween = TweenService:Create(hrp, tweenInfo, {CFrame = originalCFrame})
        tween:Play()
        tween.Completed:Wait()
    end
end

local function autoQuestFast()
    if not farmActive then return end
    teleportTo(questGiver)
    wait(0.03)
    if not farmActive then return end
    triggerPrompt(questGiver)
    wait(0.03)
    if not farmActive then return end
    teleportTo(completePart)
end

Farm500Toggle:OnChanged(function(value)
    farmActive = value
    if value then
        originalCFrame = hrp.CFrame
        screenGui.Enabled = true
    else
        screenGui.Enabled = false
        returnToOriginal()
    end
end)

task.spawn(function()
    while true do
        if farmActive then
            autoQuestFast()
            wait(0.03)
        else
            wait(0.03)
        end
    end
end)

