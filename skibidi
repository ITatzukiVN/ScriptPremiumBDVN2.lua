local function checkKey(input)
    local now = os.time()
    local uid = player.UserId
    for _,v in ipairs(keys) do
        if input == v.Key then
            if uid ~= v.UserId then
                return false, "Key khÃ´ng thuá»™c báº¡nâŒ"
            elseif now > v.Expire then
                return false, "Key Ä‘Ã£ háº¿t háº¡nâŒ"
            else
                return true
            end
        end
    end
    return false, "Key khÃ´ng há»£p lá»‡âŒ"
end

