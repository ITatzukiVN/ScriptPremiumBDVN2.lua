local function checkKey(input)
    local now = os.time()
    local uid = player.UserId
    for _,v in ipairs(keys) do
        if input == v.Key then
            if uid ~= v.UserId then
                return false, "Key kh√¥ng thu·ªôc b·∫°n‚ùå"
            elseif now > v.Expire then
                return false, "Key ƒë√£ h·∫øt h·∫°n‚ùå"
            else
                return true
            end
        end
    end
    return false, "Key kh√¥ng h·ª£p l·ªá‚ùå"
end

confirm.MouseButton1Click:Connect(function()
    local valid, reason = checkKey(textbox.Text)
    if valid then
        title.Text = "‚úÖ Key h·ª£p l·ªá! ƒêang ch·∫°y..."
        wait(0.5)
        gui:Destroy()
